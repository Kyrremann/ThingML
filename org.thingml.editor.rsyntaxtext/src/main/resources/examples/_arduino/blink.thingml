import "file:/home/kyrremann/workspace/fork/ThingML/org.thingml.samples/src/main/thingml/samples/blink.thingml"
import "file:/home/kyrremann/workspace/fork/ThingML/org.thingml.samples/src/main/thingml/hardware/bricks/_arduino/led.thingml"
import "file:/home/kyrremann/workspace/fork/ThingML/org.thingml.samples/src/main/thingml/core/_arduino/timer.thingml"

configuration BlinkArduino
{       
	group led : LedArduino
		set led.io.digital_output.pin = DigitalPin:PIN_13
	// The timer
	instance timer : TimerArduino
	// The blink application
	instance app : Blink
	connector app.HW => led.led.Led
	connector app.HW => timer.timer
}