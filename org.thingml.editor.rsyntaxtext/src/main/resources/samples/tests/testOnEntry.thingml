import "../thingml.thingml"

thing TestOnEntry includes Test 
@test "t # E"
@test "tt # EE"
@test "tdfgt # EE"
@test "t* # E*"
{
	statechart TestOnEntry init I {
	
		state I {
			transition -> J
			event m : harness?testIn
			guard m.c == '\'t\''
		}
		state J {
			on entry harness!testOut('\'E\'')
			transition -> J
			event m : harness?testIn
			guard m.c == '\'t\''
		}
	}
}